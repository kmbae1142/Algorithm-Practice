section .data
    input: db "%d", 0
    output: db "%d", 10, 0
    n: dd 0

section .text
    global main
    extern scanf
    extern printf

main:
    push rbp

    mov rdi, input
    lea rsi, [n]
    call scanf

    mov rax, 0
    mov rcx, 0
    jmp loop

loop:
    add rcx, 1
    add rax, rcx
    cmp rcx, [n]
    jne loop

    mov rdi, output
    mov rsi, rax
    call printf

    pop rbp
    mov rax, 0
    ret