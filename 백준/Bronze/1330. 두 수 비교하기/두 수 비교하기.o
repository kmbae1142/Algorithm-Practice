section .data
    input: db "%d %d", 0
    less: db "<", 10, 0
    greater: db ">", 10, 0
    equal: db "==", 10, 0
    a: dd 0
    b: dd 0

section .text
    global main
    extern scanf
    extern printf

main:
    push rbp

    mov rdi, input
    lea rsi, [a]
    lea rdx, [b]
    call scanf

    mov eax, [a]
    cmp eax, [b]

    jl L1
    je L2

    mov rdi, greater
    jmp Done

L1:
    mov rdi, less
    jmp Done

L2:
    mov rdi, equal

Done:
    call printf
    pop rbp
    mov rax, 0
    ret